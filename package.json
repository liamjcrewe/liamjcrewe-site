{
  "name": "liamjcrewe-site",
  "version": "1.0.0",
  "description": "My personal website",
  "license": "MIT",
  "author": "Liam Crewe <liam.j.crewe@gmail.com>",
  "scripts": {
    "build": "next build && next export && touch out/.nojekyll",
    "dev": "next dev",
    "lint": "standard --verbose | snazzy",
    "lint-fix": "standard --fix",
    "start": "next start",
    "deploy": "npm run build && git add out/ && git commit -m \"Deploy to GitHub pages\" && git push origin master && git subtree push --prefix out git@github.com:liamjcrewe/liamjcrewe.github.io.git master"
  },
  "dependencies": {
    "@zeit/next-css": "^1.0.1",
    "next": "^9.1.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-icons": "^3.8.0",
    "sanitize.css": "^9.0.0",
    "styled-components": "^4.2.0"
  },
  "devDependencies": {
    "babel-plugin-styled-components": "^1.10.0",
    "snazzy": "^8.0.0",
    "standard": "^12.0.1"
  }
}
